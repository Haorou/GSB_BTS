
@{
    ViewBag.Title = "Detail_RDV";
}

<h2>Detail RDV</h2>
<div class="modal fade" id="exampleModal" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel" aria-hidden="true">
    <div class="modal-dialog" role="document">
        <div class="modal-content">
            <div class="modal-header">
                <h5 class="modal-title" id="exampleModalLabel">DETAIL RDV</h5>
                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true">&times;</span>
                </button>
            </div>
            <div class="modal-body">
                <form>
                    <div class="form-group">
                        <label for="recipient-name" class="col-form-label">DETAIL RDV</label>
                        <input type="text" class="form-control" id="practicien">
                        <input type="text" class=" form-control" id="date_rdv">
                        <input type="text" class=" form-control" id="motif_rdv">
                        <input type="text" class=" form-control" id="indice_confiance">
                    </div>
                    <div class="form-group">
                        <label for="message-text" class="col-form-label">Fiche frais:</label>
                        <input type="text" class="form-control" id="hebergement">
                        <input type="text" class=" form-control" id="restauration">
                        <input type="text" class=" form-control" id="deplacement">
                        <input type="text" class=" form-control" id="total">
                    </div>
                </form>
            </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-secondary" data-dismiss="modal">Close</button>
                <button type="button" class="btn btn-primary">Envoyer</button>
            </div>
        </div>
    </div>
</div>
